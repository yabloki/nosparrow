    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "attach",
            "name": "attach to container",
            "port": 9229,
            "skipFiles": [
                "<node_internals>/**"
            ],
            "localRoot": "${workspaceFolder}/metaauth"
            "remoteRoot": "/usr/src/app"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "metaauth api",
            "program": "${workspaceFolder}/metaauth/bin/www",
            "skipFiles": [
                "<node_internals>/**"
            ]
        }
    ]
}